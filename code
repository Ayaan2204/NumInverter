#include <stdio.h>
#include <conio.h>
void main()
{
    //VARIABLES
    int a;
    int quotient;
    //INPUT
    printf("Enter Number \n");
    scanf("%d",&a);
    //DIGITCOUNT
    quotient=a;
    while(quotient>0)
    {
        printf("%d",quotient%10);
        quotient=quotient/10;
        
    }
}   
